{
	"name": "people-notes",
	"version": "1.0.0",
	"description": "Create timestamped notes for people in organized directories with automatic embedding",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs build",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --ext .ts --fix",
		"typecheck": "tsc --noEmit"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"people",
		"notes",
		"organization"
	],
	"author": "Your Name",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"eslint": "8.18.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"obsidian": "latest",
		"ts-jest": "^29.1.2",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}